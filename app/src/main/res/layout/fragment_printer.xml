<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollIndicators="right"
    android:scrollbars="vertical"
    tools:context="com.seaory.seaorydemo.PrinterFragment"
    tools:targetApi="m">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="2">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:orientation="horizontal"
                            android:layout_weight="1">
                                <Button
                                    android:id="@+id/btn_connectPrinter"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:text="@string/connect_printer"
                                    android:layout_weight="1"
                                    android:textColor="@android:color/holo_red_dark"
                                    />
                                <Button
                                    android:id="@+id/btn_disconnectPrinter"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:text="@string/disconnect_printer"
                                    android:layout_weight="1"
                                    android:textColor="@android:color/holo_red_dark"
                                    />
                                <CheckBox
                                    android:id="@+id/checkBox_useIP"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/use_ip"/>

                                <EditText
                                    android:id="@+id/editTextIP"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:ems="10"
                                    android:inputType="textWebEmailAddress"
                                    android:text="192.168.0.5"/>

                        </LinearLayout>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="@android:color/black"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:orientation="horizontal"
                            android:layout_weight="1">
                                <Button
                                    android:id="@+id/btn_demo"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/Demo"
                                    />
                                <Button
                                    android:id="@+id/btn_SetLogLevel"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5"
                                    android:text="@string/SetLogLevel"
                                    />
                                <Button
                                    android:id="@+id/btn_getPrinterStatus"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5"
                                    android:text="@string/get_printer_status"
                                    />
                                <Button
                                    android:id="@+id/btn_getPrinterWarning"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5"
                                    android:text="@string/get_printer_warning"
                                    />
                        </LinearLayout>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="@android:color/black"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:orientation="horizontal"
                            android:layout_weight="1">
                                <Button
                                    android:id="@+id/btn_getPrinterInfo"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/get_printer_info"
                                    />
                                <Spinner
                                    android:id="@+id/spinner_printerInfo"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="3" />

                        </LinearLayout>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="@android:color/black"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:orientation="horizontal"
                            android:layout_weight="1">
                                <Button
                                    android:id="@+id/btn_moveCard"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/move_card_to"
                                    android:textColor="@android:color/holo_red_dark"
                                    />
                                <Spinner
                                    android:id="@+id/spinner_cardPos"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="3"
                                    android:spinnerMode="dialog"
                                    />
                        </LinearLayout>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="@android:color/black"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:orientation="horizontal"
                            android:layout_weight="1">
                                <Button
                                    android:id="@+id/btn_execCommand"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/execute_command"
                                    android:textColor="@android:color/holo_red_dark"
                                    />
                                <Spinner
                                    android:id="@+id/spinner_printerCommand"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="3"
                                    android:spinnerMode="dialog"
                                    />

                        </LinearLayout>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="@android:color/black"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:orientation="horizontal"
                            android:layout_weight="2">
                                <Button
                                    android:id="@+id/btn_setCardInOut"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/set_card_input_output"
                                    android:textColor="@android:color/holo_red_dark"
                                    />
                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical"
                                    android:layout_weight="1">
                                        <Spinner
                                            android:id="@+id/spinner_cardInput"
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1"
                                            />
                                        <Spinner
                                            android:id="@+id/spinner_cardOutput"
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1"
                                            />
                                        <Spinner
                                            android:id="@+id/spinner_cardOutput2"
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1"
                                            />
                                </LinearLayout>
                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical"
                                    android:layout_weight="1">
                                        <CheckBox
                                            android:id="@+id/checkBox_autoFeed"
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1"
                                            android:text="@string/auto_feed_card_when_card_is_detected"
                                            />

                                        <CheckBox
                                            android:id="@+id/checkBox_waitRemoval"
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1"
                                            android:text="@string/wait_for_removal_after_printing"
                                            />
                                        <CheckBox
                                            android:id="@+id/checkBox_auto_eject"
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1"
                                            android:text="@string/auto_eject"
                                            />
                                </LinearLayout>
                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical"
                                    android:layout_weight="1">
                                        <CheckBox
                                            android:id="@+id/checkBox_Cardout"
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1"
                                            android:text="@string/enable_card_out_sensor"
                                            />

                                        <CheckBox
                                            android:id="@+id/checkBox_Rejectbox"
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1"
                                            android:text="@string/enable_reject_box_sensor"
                                            />
                                </LinearLayout>
                        </LinearLayout>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="@android:color/black"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:orientation="horizontal"
                            android:layout_weight="1">
                                <Button
                                    android:id="@+id/btn_setStandbyPara"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/set_standby_parameters"
                                    />

                                <Spinner
                                    android:id="@+id/spinner_printerEnd"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:contentDescription="@string/auto_feed_card_when_card_is_detected" />

                                <EditText
                                    android:id="@+id/editTextStandbyPos"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.6"
                                    android:ems="10"
                                    android:inputType="numberDecimal"
                                    android:gravity="center"
                                    />

                                <TextView
                                    android:id="@+id/textView"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.4"
                                    android:gravity="center_vertical"
                                    android:text="mm"
                                    />

                                <EditText
                                    android:id="@+id/editTextStandbyTime"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.6"
                                    android:ems="10"
                                    android:gravity="center"
                                    android:inputType="numberDecimal"
                                    android:autofillHints="" />

                                <TextView
                                    android:id="@+id/textView2"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.4"
                                    android:gravity="left|center_vertical"
                                    android:text="@string/x0_5sec"
                                    />

                        </LinearLayout>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="@android:color/black"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:orientation="horizontal"
                            android:layout_weight="1">
                                <Button
                                    android:id="@+id/btn_printOneCard"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/print_one_card"
                                    android:textColor="@android:color/holo_red_dark"
                                    />
                                <Button
                                    android:id="@+id/btn_printDemo"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/print_demo_card"
                                    android:textColor="@android:color/holo_red_dark"
                                    />
                                <Button
                                    android:id="@+id/btn_SimplePrint"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/simple_print"
                                    android:textColor="@android:color/holo_red_dark"
                                    />
                                <Spinner
                                    android:id="@+id/spinner_printSide"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:orientation="horizontal"
                            android:layout_weight="1">

                                <TextView
                                    android:id="@+id/textViewHeatK"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:text="@string/heat_energy_k"
                                    />

                                <Spinner
                                    android:id="@+id/spinner_heatEnergyFrontK"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    />

                                <Spinner
                                    android:id="@+id/spinner_heatRespFrontK"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    />

                        </LinearLayout>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="@android:color/black"/>
                        <Space
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:orientation="horizontal"
                            android:layout_weight="1"/>
                </LinearLayout>
        </LinearLayout>
</android.support.constraint.ConstraintLayout>
